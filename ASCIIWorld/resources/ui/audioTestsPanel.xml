<StackPanel x="32" y="32" width="240" height="384" orientation="Vertical">
	<Children>
		<Button text="Tones" margin="10">
			<Button.click>
	        	gen = new Packages.asciiWorld.audio.SignalGenerator();
	        	gen.generate(Packages.asciiWorld.audio.SignalType.Tones);
				new Packages.asciiWorld.audio.AudioPlaybackThread(gen).start();
			</Button.click>
